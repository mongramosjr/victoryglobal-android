<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!-- template for profile for each field -->
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        >

        <LinearLayout
            android:layout_width="64dp"
            android:layout_height="match_parent"
            android:gravity="center_horizontal|center_vertical"
            android:background="@color/materialGrey_100">
            <ImageView
                android:src="@drawable/cake_variant"
                android:layout_width="32dp"
                android:layout_height="32dp"
                />
        </LinearLayout>

        <View
            style="@style/ViewVerticalSeparator"
            android:background="@color/colorNeutralWarmLighter"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:layout_marginStart="@dimen/container_margin_horizontal"
            android:layout_marginEnd="@dimen/container_margin_horizontal"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingBottom="@dimen/container_padding_vertical"
                android:paddingTop="@dimen/container_padding_vertical"
                android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Date of Birth"
                android:textAppearance="@style/TextAppearance.AppCompat.Small"
                />

            <TextView
                android:id="@+id/date_of_birth"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAppearance="@style/TextAppearance.AppCompat.Subhead"
                />

            </LinearLayout>

            <View
                style="@style/ViewSeparator"
                android:layout_gravity="bottom"
                android:background="@color/colorNeutralWarmLighter"/>

        </LinearLayout>

    </LinearLayout>

